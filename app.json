{
    "expo": {
      "name": "power-fittness",
      "slug": "power-fittness",
      "version": "1.0.0",
      "platforms": ["ios", "android"],
      "orientation": "portrait",
      "icon": "./assets/icon.png", 
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "scheme": "powerfittness", 
      "ios": {
        "bundleIdentifier": "com.miempresa.powerfittness",
        "buildNumber": "1",
        "supportsTablet": true
      },
      "android": {
        "package": "com.miempresa.powerfittness",
        "versionCode": 1,
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        }
      },
      "assetBundlePatterns": [
        "assets/images/*"
      ],
      "updates": {
        "enabled": true
      }
    }
  }
  